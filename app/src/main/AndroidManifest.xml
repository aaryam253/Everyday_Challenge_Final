<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aaryam123.everyday_challenge_final">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Ultimate_Activity"
            android:label="@string/title_activity_ultimate_" />
        <activity
            android:name=".Progress_Activity"
            android:label="@string/title_activity_progress_" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".challengelevel1" />
        <activity android:name=".challengelevel2" />
        <activity android:name=".challengelevel3" />
        <activity android:name=".challengelevel4" />
        <activity android:name=".challengelevel5" />
        <activity android:name=".challengelevel6" />
        <activity android:name=".challengelevel7" />
        <activity android:name=".challengelevel8" />
        <activity android:name=".challengelevel9" />
        <activity android:name=".profile"></activity>
    </application>

</manifest>